{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "redux-saga"
  ],
  "rules": {
    "max-len": [2, 120],
    "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/client.babel.js"
      }
    }
  }
}
